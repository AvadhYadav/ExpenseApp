<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.vasu.expense_manager">

    <uses-permission android:name="android.permission.INTERNET"></uses-permission>

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_debt"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.example.vasu.expense_manager.LoginActivity"
            android:label="@string/app_name"
            android:screenOrientation="portrait"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name="com.example.vasu.expense_manager.MainActivity"></activity>
        <activity android:name="org.achartengine.GraphicalActivity" />
        <activity
            android:name="com.example.vasu.expense_manager.TransactionActivity"
            android:label="@string/title_activity_transaction"
            android:parentActivityName="com.example.vasu.expense_manager.MainActivity"
            android:theme="@style/AppTheme.NoActionBar">
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value="com.example.vasu.expense_manager.MainActivity" />
        </activity>
    </application>

</manifest>
